{
  "name": "page-transformer",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "ssrdev": "node server.mjs",
    "ssr": "env NODE_ENV=production node server.mjs",
    "build": "vite build",
    "buildssr": "vite build --outDir dist/server --ssr src/entry-server.js",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "express": "^4.18.1",
    "serve-static": "^1.15.0",
    "vite-plugin-externals": "^0.5.0",
    "vue": "^3.2.36"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.0.0",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "vite": "^2.9.9",
    "vitest": "^0.13.0"
  }
}
